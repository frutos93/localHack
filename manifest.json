{
    "manifest_version": 2,

    "name": "Bullying to jokes",
    "version": "1.0.0",
    "description": "Anti bullying extension that changes tweets or facebook posts that contain harrasment material.",
    "permissions":["https://partner.bark.us/*", "https://tambal.azurewebsites.net/*", "https://api.icndb.com/*"],
    "browser_action": {
    },
    "background": { "scripts": ["background.js"] },
    "content_scripts": [{
        "js": ["jquery.min.js", "content.js"],
        "matches": ["https://twitter.com/*", "https://www.facebook.com/*"]
    }]

}